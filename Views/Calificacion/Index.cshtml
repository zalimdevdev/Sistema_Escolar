@model IEnumerable<Sis_Escolar.Models.Calificacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Calificaciones</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Estudiante)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Asignatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Not)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nota)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Estudiante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asignatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Not)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nota)
            </td>
            <td>
                <div class="btn-group" role="group" aria-label="Basic example">
                <a type="button" class="btn btn-primary" asp-action="Edit" asp-route-id="@item.id">Edit</a>
                <a type="button" class="btn btn-success" asp-action="Details" asp-route-id="@item.id">Details</a>
                <a type="button" class="btn btn-danger" asp-action="Delete" asp-route-id="@item.id">Delete</a>

                </div>
            </td>
        </tr>



        
}
    </tbody>
</table>
